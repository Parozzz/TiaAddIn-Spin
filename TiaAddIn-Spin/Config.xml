<PackageConfiguration xmlns="http://www.siemens.com/automation/Openness/AddIn/Publisher/V16">
  <Author>Siemens AG - SIMATIC Systems Support</Author>
  <AddInVersion>V16</AddInVersion>
  <Description>With this TIA Add-In you can convert a FC into a FB and vice versa.</Description>

  <Product>
    <Name>SpinAddIn</Name>
    <Id>fspin-addin</Id>
    <Version>0.0.1</Version>
  </Product>

  <FeatureAssembly>
    <AssemblyInfo>
      <Assembly>bin\Release\SpinAddIn.dll</Assembly>
    </AssemblyInfo>
  </FeatureAssembly>

  <AdditionalAssemblies>
    <AssemblyInfo>
      <Assembly>bin\Release\SpinAddIn.XmlSerializers.dll</Assembly>
    </AssemblyInfo>
  </AdditionalAssemblies>

  <RequiredPermissions>
    <TIAPermissions>
      <TIA.ReadWrite/>
    </TIAPermissions>
    <SecurityPermissions>
      <System.Security.Permissions.FileIOPermission/>
      <System.Security.Permissions.EnvironmentPermission/>
      <System.Security.Permissions.UIPermission/>
      <System.Security.Permissions.SecurityPermission.UnmanagedCode/>
    </SecurityPermissions>
  </RequiredPermissions>
</PackageConfiguration>